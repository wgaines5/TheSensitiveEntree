<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <TextView
        android:id="@+id/greenBar3_textView"
        android:layout_width="375dp"
        android:layout_height="8dp"
        android:layout_marginTop="20dp"
        android:background="@color/dark_green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/selectSub_textView" />

    <TextView
        android:id="@+id/ingredientSub3_textView"
        android:layout_width="319dp"
        android:layout_height="34dp"
        android:layout_marginTop="308dp"
        android:background="@color/off_white"
        android:fontFamily="@font/playfair_display"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/selectSub_textView" />

    <TextView
        android:id="@+id/ingredientSub4_textView"
        android:layout_width="319dp"
        android:layout_height="34dp"
        android:layout_marginTop="440dp"
        android:background="@color/off_white"
        android:fontFamily="@font/playfair_display"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/selectSub_textView" />

    <TextView
        android:id="@+id/ingredientSub5_textView"
        android:layout_width="319dp"
        android:layout_height="34dp"
        android:layout_marginTop="180dp"
        android:background="@color/off_white"
        android:fontFamily="@font/playfair_display"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/selectSub_textView" />

    <TextView
        android:id="@+id/ingredientSub6_textView"
        android:layout_width="319dp"
        android:layout_height="34dp"
        android:layout_marginTop="52dp"
        android:background="@color/off_white"
        android:fontFamily="@font/playfair_display"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/selectSub_textView" />

    <TextView
        android:id="@+id/selectSub_textView"
        android:layout_width="232dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/playfair_display"
        android:text="Select Ingredient"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ingredSub_toolbar" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/ingredSub_toolbar"
        android:layout_width="411dp"
        android:layout_height="51dp"
        android:background="@color/light_green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>